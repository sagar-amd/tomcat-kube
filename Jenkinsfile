pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello, Welcome to INDIA :)'
            }
        }
        stage('stage1') {
            steps {
                echo "This is awsome !!!"
            }
        }
        stage('stage2') {
            steps {
                echo "WebHook No Triggered !!!"
            }
        }
        stage('stage3') {
            steps {
                echo "WebHook Triggered Now !!!"
            }
    }
}
